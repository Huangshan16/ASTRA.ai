{
  "type": "extension",
  "name": "reecho_tts",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.1.0"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "src/**.tent",
      "src/**.py",
      "README.md"
    ]
  },
  "api": {
    "property": {
        "voice_id": {
            "type": "string"
        },
        "model": {
            "type": "string"
        },
        "prompt_id": {
            "type": "string"
        },
        "randomness": {
            "type": "int64"
        },
        "stability_boost": {
            "type": "int64"
        },
        "probability_optimization": {
            "type": "int64"
        },
        "break_clone": {
            "type": "bool"
        },
        "flash": {
            "type": "bool"
        },
        "stream": {
            "type": "bool"
        },
        "seed": {
            "type": "int64"
        },
        "api_url": {
            "type": "string"
        }
    },
    "data_in": [
        {
            "name": "text_data",
            "property": {
                "text": {
                    "type": "string"
                }
            }
        }
    ],
    "cmd_in": [
        {
            "name": "flush"
        }
    ],
    "cmd_out": [
        {
            "name": "flush"
        }
    ],
    "audio_frame_out": [
        {
            "name": "pcm_frame"
        }
    ]
  }
}